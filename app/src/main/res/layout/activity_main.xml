<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical">

    <android.support.v7.widget.Toolbar
        android:id="@+id/logintoolbar"
        android:layout_width="match_parent"
        android:layout_height="?actionBarSize"
        android:background="#9EC0E6"
        app:titleTextColor="@android:color/white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_gravity="center"
            android:text="X5WebView 简 介"
            android:textColor="#808080"
            android:textSize="19sp" />
    </android.support.v7.widget.Toolbar>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#e7e7e7" />

    <TextView
        android:id="@+id/myinfo"
        style="@style/useText"
        android:text=" 简    书  " />

    <TextView
        android:id="@+id/baidu"
        style="@style/useText"
        android:text=" 百    度 " />
    <TextView
        android:id="@+id/interview"
        style="@style/useText"
        android:text="Android   面 试 总 结" />

    <TextView
        android:id="@+id/gamesdk"
        style="@style/useText"
        android:text="Android 游 戏 SDK 介 绍" />
    <TextView
        android:id="@+id/x5"
        style="@style/useText"
        android:text="X5WebView基 本 简 介" />
    <TextView
        android:id="@+id/butterknife"
        style="@style/useText"
        android:text="自 定 义 butterknife 框 架" />

    <TextView
        android:id="@+id/synCookie"
        style="@style/useText"
        android:text="客 户 端 与 H5 同 步 Cookie" />
    <TextView
        android:id="@+id/morechannel"
        style="@style/useText"
        android:text="多渠道打包 AndroidV1、V2签名包说明" />


</LinearLayout>
